{
  "disabled": false,
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "info",
      "webHookType": "genericJson",
      "authLevel": "anonymous",
      "methods": [
        "post"
      ]
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "blobContents",
      "path": "strings/{BlobName}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "entryPoint": "handle",
  "scriptFile": "../.webpack/service/src/handlers/http/getfilecontent.js"
}