{
  "disabled": false,
  "bindings": [
    {
      "type": "queueTrigger",
      "direction": "in",
      "name": "myQueueItem",
      "queueName": "myqueue-items2",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "myInputBlob",
      "path": "samples-workitems2/{queueTrigger}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "myOutputBlob",
      "path": "samples-workitems2/{queueTrigger}-Copy",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "entryPoint": "handle",
  "scriptFile": "../.webpack/service/src/handlers/storage/storageQueue2.js"
}