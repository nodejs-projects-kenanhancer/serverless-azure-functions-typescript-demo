{
  "name": "serverless-azure-functions-typescript-demo",
  "version": "1.0.0",
  "description": "Azure Functions webpack sample for the Serverless framework using Typescript",
  "scripts": {
    "dev": "npx azurite --silent & npm start",
    "start": "sls offline",
    "prestart": "npm run build",
    "build": "sls offline build",
    "deploy": "sls deploy --stage dev",
    "test": "echo \"No tests yet...\"",
    "test2": "sls invoke local --port 7071 --function hello --data '{\"name\":\"kenan\"}'",
    "test3": "curl http://localhost:7071/api/hello?name=kenan"
  },
  "keywords": [
    "azure",
    "serverless"
  ],
  "devDependencies": {
    "@azure/functions": "^1.2.3",
    "@types/node": "^14.14.40",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "serverless-azure-functions": "^2.1.0",
    "serverless-offline": "^6.9.0",
    "serverless-webpack": "^5.4.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.33.2",
    "webpack-node-externals": "^2.5.2"
  }
}
